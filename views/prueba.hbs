{{appName}} <br>

{{#unless usuarios}}
    <p>No existen usuarios</p>
{{/unless}}

{{#if usuarios.length}}
    {{log "Existen usuarios"}}
    {{#each usuarios}}
        {{id}}:{{name}}, <br>
    {{/each}}
    {{else}}
        <p>No existen usuarios</p>
{{/if}}

{{#with administrador}}
    Nombre: {{nombre}}, Apellido: {{apellidos}}
{{else}}
    No existe el administrador
{{/with}}